  "name": "NGDotNet",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashlinallen/NGDotNet.com.git"
  },
  "version": "1.0.0",
  "description": "A demonstration scaffold for building AngularJS apps using Webpack, C#/.NET and Entity Framework 6.",
  "main": "bundle.js",
  "scripts": {
    "Run - Development": "webpack -d --color",
    "Run - Production": "webpack -p --color",
    "Profile - Development": "webpack -d --profile --json > stats.json",
    "Profile - Production": "webpack -p --profile --json > stats.json",
    "Serve Hot - Development": "webpack-dev-server --hot --color",
    "Serve Cold - Production": "webpack-dev-server --color",
    "Watch - Development": "webpack -d --watch --color",
    "Watch - Production": "webpack -p --watch --color",
    "Prod": "webpack -p --config webpack.config.js --output-public-path /dist/",
    "Test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ashlin Allen",
  "bugs": {
    "url": "https://github.com/ashlinallen/NGDotNet.com/issues"
  },
