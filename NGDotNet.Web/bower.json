  "keywords": [
    "AngularJS",
    ".NET",
    "EF6",
    "Gulp",
    "Browserify",
    "Visual Studio"
  ],
  "authors": [
    "Ashlin Allen"
  ],
  "license": "",
  "homepage": "http://blog.ashlinallen.com/",
  "ignore": [
    "**/.*",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.4.8",
    "angular-animate": "~1.4.8",
    "angular-route": "~1.4.8",
    "angular-sanitize": "~1.4.8",
    "angular-bootstrap": "~0.14.3",
    "extras.angular.plus": "~0.9.3",
    "jquery": "~2.1.4",
    "moment": "~2.10.6",
    "bootstrap": "~3.3.6",
    "font-awesome": "~4.5.0",
    "toastr": "~2.1.2"
